    "\n",
    "# Python Program for Even or Odd Checker\n",
    "user_number = int(input(\"Enter an integer: \"))\n",
    "\n",
    "if user_number % 2 == 0:\n",
    "    print(f\"{user_number} is an even number.\")\n",
    "else:\n",
    "    print(f\"{user_number} is an odd number.\")\n"
   ]
